<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.abc.my_project.second"
    android:background="@drawable/back">


    <Button
        android:id="@+id/button23"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="112dp"
        android:text="Microprocessor and Interfacing" />

    <Button
        android:id="@+id/button24"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button23"
        android:layout_alignStart="@+id/button23"
        android:layout_below="@+id/button23"
        android:layout_marginTop="25dp"
        android:text="Operating Systems" />

    <Button
        android:id="@+id/button25"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button24"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        android:text="Theory Of Computation" />

    <Button
        android:id="@+id/button26"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button25"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="24dp"
        android:text="Image Processing" />

    <Button
        android:id="@+id/button27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button26"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="33dp"
        android:text="Other Courses" />
</RelativeLayout>
